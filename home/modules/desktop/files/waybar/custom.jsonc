    //   "false": "ó°›§"
    // },
  },
  "custom/idasen": {
    "format": "{}",
    "on-click": "curl -X POST http://192.168.178.69:8123/api/webhook/-iEw_PiBC1f3dE3MTcOo6Xqle",
    "exec": "curl http://192.168.178.69:8123/api/ > /dev/null 2>&1 && echo 'ó±ˆ¹' || echo ''",
    "interval": "60"
  },
  "custom/shutdown": {
    "format": "ï€‘",
    "tooltip-format": "Shut It DOWN ðŸ™…",
    "on-click": "shutdown -h now"
  },
  "custom/reboot-linux": {
    "format": "î«’",
    "tooltip-format": "Reboot",
    "on-click": "test $HOST = terra && reboot-linux || reboot"
  },
  "custom/reboot-windows": {
    "format": "î˜ª",
    "tooltip-format": "Reboot into Windows",
    "on-click": "reboot-windows",
    "exec-if": "test $HOST = terra"
  },
  "custom/mail": {
    "format": "{}",
    "exec": "$HOME/.config/waybar/mail count-all",
    "return-type": "json",
    "interval": 60,
    "on-click": "kitty aerc"
  },
  "custom/brightness#c34": {
    "format": "{}",
    "tooltip-format": "C34J79x Brightness",
    "exec": "$HOME/.config/waybar/brightness get C34",
    "on-scroll-up": "$HOME/.config/waybar/brightness set C34 + 10",
    "on-scroll-down": "$HOME/.config/waybar/brightness set C34 - 10"
    // "interval": 1
  },
  "custom/undome": {
    "format": "{}",
    "exec": "$HOME/.config/waybar/undome capacity",
    "interval": 1
  },
  "custom/module-left#player": {
    "format": " ",
    "exec": "playerctl metadata --all-players",
    "interval": 1
  },
  "custom/player-title": {
    "format": "{}",
    "exec": "playerctl metadata --all-players --follow --format '{{title}}'",
    "on-click": "playerctl play-pause"
  },
  "custom/player-position": {
    "format": "{}",
    "exec": "$HOME/.config/waybar/player-position.sh",
    "interval": 1
  },
  "custom/veil": {
    "format": "{}",
    "exec": "$HOME/.config/waybar/quicksetting.sh veil",
    "on-click": "test -f /tmp/veil.pid && kill $(cat /tmp/veil.pid) || $HOME/.config/eww/scripts/veil"
  }
